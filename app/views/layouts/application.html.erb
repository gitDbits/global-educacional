        <div class="container custom-container">
          <div class="row">
            <div class="col-12">
              <div class="mobile-nav-toggler"><i class="tg-flaticon-menu-1"></i></div>
              <div class="tgmenu__wrap">
                <nav class="tgmenu__nav">
                  <div class="logo">
                    <%= link_to root_pt_path do %>
                      <img src="assets/img/logo/logo.png" alt="logo">
                    <% end %>
                  </div>
                  <div class="tgmenu__navbar-wrap tgmenu__main-menu d-none d-xl-flex">
                    <ul class="navigation">
                      <li class="menu-item">
                        <a href="#cursos"><strong>Cursos</strong></a>
                      </li>
                      <li class="menu-item">
                        <a href="#sobre_nos"><strong>Sobre nós</strong></a>
                      </li>
                    </ul>
                  </div>
                  <div class="tgmenu__action">
                    <ul class="list-wrap">
                      <li class="header-phone d-none d-lg-block d-xl-none d-xxl-block">
                        <a href="https://api.whatsapp.com/send?phone=5569992729043" target="_blank">
                          <i class="mdi mdi-whatsapp"></i> (69) 99272-9043
                        </a>
                      </li>
                      <li class="header-btn">
                        <a href="https://globaleducacional.jacad.com.br/" class="btn" title="Acessar portal">
                          <i class="fas fa-laptop"></i> Acessar portal</a>
                      </li>
                    </ul>
                  </div>
                </nav>
              </div>
              <!-- Mobile Menu  -->
              <div class="tgmobile__menu">
                <nav class="tgmobile__menu-box">
                  <div class="close-btn"><i class="tg-flaticon-close-1"></i></div>
                  <div class="nav-logo">
                    <a href="index.html"><img src="assets/img/logo/logo.png" alt="Logo"></a>
                  </div>
                  <div class="tgmobile__search tgmenu__action">
                    <ul class="list-wrap">
                      <li class="header-btn login-btn">
                        <li class="header-btn">
                          <a href="https://ead.globaleducacional.com.br/" class="btn" target="_blank">
                            <i class="fas fa-laptop"></i> Área do Aluno
                          </a>
                        </li>
                      </li>
                    </ul>
                  </div>
                  <br>
                  <div class="tgmobile__menu-outer">
                    <!--Here Menu Will Come Automatically Via Javascript / Same Menu as in Header-->
                  </div>
                  <div class="tgmenu__action">
                    <ul class="list-wrap">
                      <li class="header-btn">
                        <a href="https://api.whatsapp.com/send?phone=5569992729043">
                          <i class="mdi mdi-whatsapp mdi-18px"></i> (69) 984482693
                        </a>
                      </li>
                    </ul>
                  </div>
                  <div class="social-links">
                    <ul class="list-wrap">
                      <li><a href="https://www.instagram.com/globaleducacional/"><i class="fab fa-instagram"></i></a></li>
                    </ul>
                  </div>
                </nav>
              </div>
              <div class="tgmobile__menu-backdrop"></div>
              <!-- End Mobile Menu -->
            </div>
          </div>
        </div>
      </div>
    </header>
    <!-- header-area-end -->
    <%= yield %>
